  object qryFonte: TADOQuery
    Active = True
    Connection = dmtData.adoCon
    CursorType = ctStatic
    DataSource = sQryTot
    Parameters = <
      item
        Name = 'recurso'
        Attributes = [paNullable]
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 1
        Value = '2'
      end>
    Prepared = True
    SQL.Strings = (
      'SELECT recurso, fonte, fonteD = f.descricao,'
      '       total = SUM(liquida),'
      '       fiscal = SUM(liqFiscal),'
      '       seguridade = SUM(liqSeguridade)'
      '  FROM receita r'
      '   INNER JOIN fonte f ON r.fonte = f.codigo'
      '     WHERE recurso = :recurso'
      '     GROUP BY recurso, fonte, f.descricao'
      '     ORDER BY fonte')
    Left = 40
    Top = 8
  end
  object sQryFonte: TDataSource
    DataSet = qryFonte
    Left = 40
    Top = 40
  end
  object qry: TADOQuery
    Active = True
    Connection = dmtData.adoCon
    CursorType = ctStatic
    DataSource = sQryFonte
    Parameters = <
      item
        Name = 'recurso'
        Attributes = [paNullable]
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 1
        Value = '2'
      end
      item
        Name = 'fonte'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 3
        Value = '280'
      end>
    Prepared = True
    SQL.Strings = (
      'SELECT recurso, fonte, destinacao,'
      '       total = SUM(liquida),'
      '       fiscal = SUM(liqFiscal),'
      '       seguridade = SUM(liqSeguridade)'
      '  FROM receita '
      '     WHERE recurso = :recurso AND fonte = :fonte'
      '     GROUP BY recurso, fonte, destinacao'
      '     ORDER BY destinacao'
      '')
    Left = 72
    Top = 8
  end
