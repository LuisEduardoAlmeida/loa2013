  object qryConsol: TADOQuery
    Active = False
    Connection = dmtData.adoCon
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select o.unorc, o.unorcd, o.descricao, l.OP, sum(o.valor) '
      'from orcamento o'
      'left outer join locali l on o.locali=l.codigo'
      'where OP ='#39'S'#39
      'GROUP BY o.unorc, o.unorcd, o.descricao, l.OP'
      'order by o.unorc, o.unorcd, o.descricao, l.OP, sum(o.valor) ')
    Left = 248
    Top = 32
    object qryConsolunorc: TStringField
      FieldName = 'unorc'
      Size = 5
    end
    object qryConsolunorcd: TStringField
      FieldName = 'unorcd'
      Size = 255
    end
    object qryConsoldescricao: TStringField
      FieldName = 'descricao'
      Size = 255
    end
    object qryConsolOP: TStringField
      FieldName = 'OP'
      Size = 1
    end
    object qryConsolCOLUMN1: TBCDField
      FieldName = 'COLUMN1'
      ReadOnly = True
      Precision = 19
    end
  end
  object qrySoma: TADOQuery
    Active = False
    Connection = dmtData.adoCon
    CursorType = ctStatic
    DataSource = dsConsol
    Parameters = <
      item
        Name = 'unorc'
        Attributes = [paNullable]
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 5
        Value = Null
      end>
    SQL.Strings = (
      'select sum(valor) '
      'from orcamento o'
      'left outer join locali l  on o.locali=l.codigo'
      'where o.descricao like '#39'% op%'#39' AND l.OP ='#39'S'#39' and o.unorc =:unorc')
    Left = 296
    Top = 36
    object qrySomaCOLUMN1: TBCDField
      FieldName = 'COLUMN1'
      ReadOnly = True
      Precision = 19
    end
  end
